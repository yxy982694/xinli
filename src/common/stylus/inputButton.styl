@import "./variable";
@import "./mixin";
// 登录页面按钮样式
.login-container .el-form-item__content
  display: flex
.login-container .el-input__inner
  height: 35px
  line-height: 35px
.login-container .el-button
  padding: 10px 20px
.el-button-group
  margin: 2px 0
.el-table .cell
  line-height: 20px
  font-size: $font-size-small
  no-wrap()
th .cell
  line-height: 25px
.el-table th
  padding: 0
.el-table td
  padding: 1px 0
// 资源管理中单元格样式
.resource-container .el-table .cell
  line-height: 25px
.resource-container th .cell
    line-height: 30px
// input框的样式
.el-input
  width: 200px
.el-input__inner
 height: 25px
 line-height: 25px
 width: 180px
 font-size: $font-size-small
.is-in-pagination .el-input__inner
  width: 35px
  border-radius: 0
.el-form-item
  display: flex
  padding-right: 20px
.el-dialog
  z-index: 8989898989898
// 弹框中input的样式
.el-form-item__content
  margin-left: 0 !important
  flex:1
  line-height: 22px
  .el-input
    width: 100%
    .el-input__inner
      width: 100%
.el-radio__label
  padding-left: 6px
.el-radio
  margin-right: 15px
  // 日期时间控件
.el-range-editor.el-input__inner
  margin: 0 20px
.el-date-editor .el-range__icon
.el-date-editor .el-range-separator
  display: flex
  align-items: center
  justify-content: center
 // 分页样式
.content-radius .el-pagination
   display: flex
   align-items: center
.content-radius .is-in-pagination
   height: 25px
.content-radius .el-pagination__jump
   height: 25px !important
   line-height: 25px !important
.content-radius .el-input__inner
   width: 25px !important
   border-radius: 2px !important
.el-pagination.is-background.el-pagination--small .btn-next, .el-pagination.is-background.el-pagination--small .btn-prev, .el-pagination.is-background.el-pagination--small .el-pager li
   width: 25px
   height: 25px
   border-radius: 2px
   background-color: #fff
   color: $color-textblack
   border: 1px solid $color-border
.el-pagination--small .btn-next, .el-pagination--small .btn-prev, .el-pagination--small .el-pager li, .el-pagination--small .el-pager li.btn-quicknext, .el-pagination--small .el-pager li.btn-quickprev, .el-pagination--small .el-pager li:last-child
   line-height: 25px !important
.el-pagination.is-background .el-pager li:not(.disabled).active
   background-color: #fff
   color: $color-textblue
   border: 1px solid $color-textblue
.el-pagination__sizes .el-input__inner
  width: 100px
.el-pagination__total
  height: 25px !important
  line-height: 25px !important
.el-pagination__sizes
.el-pagination__sizes .el-select
.el-pagination__sizes .el-select .el-input
  height: 25px !important
  line-height: 25px !important
.el-pagination button, .el-pagination span:not([class*=suffix])
  height: 25px !important
  line-height: 25px !important
  min-width: 25px !important
  color: #303133
  font-size: $font-size-small
  // font-weight: 700
.el-pagination__sizes .el-input .el-input__inner
  font-size: $font-size-small
.el-pager li
  height: 25px !important
  line-height: 25px !important
  min-width: 25px !important
  font-size: $font-size-small
  // margin-left: 5px
.el-pager+.btn-next
  // margin-left: 5px
  // 新增编辑弹框样式
.el-dialog__body .el-form-item
  display: flex
  align-items: center
.el-table .sort-caret.ascending
  top: 0
.el-table .sort-caret.descending
  bottom: 3px
.el-table .caret-wrapper
  height: 25px
.el-pagination .el-select .el-input .el-input__inner
  color: #303133
  border-radius: 0
  border: none !important
  // font-weight: 700
.el-pagination__jump
  position: relative
.el-pagination__jump::before
  content: '第'
  font-size: 13px
  display: inline-block
  text-align: right
  width: 28px
  height: 25px
  position: absolute
  left: 0
  top: 0
  background-color: #DFDFDF
.upcom-page .el-pagination__jump::before
  content: '第'
  font-size: 13px
  display: inline-block
  text-align: right
  width: 28px
  height: 25px
  position: absolute
  left: 0
  top: 0
  background-color: #fff
.el-pagination__editor
  height: 25px !important
.el-table__row .el-button--mini
.el-table__row .el-button--mini.is-round
  padding: 3px 15px
.el-tree>.el-tree-node
    min-width:100%
    display: inline-block
.menu-location .el-form-item__content
   display: flex
   align-items: center
   justify-content: space-between
.menu-location input
   flex: 1
   margin-right: 20px
   // 树形结构
.el-tree-node__label
  font-size: $font-size-small
// 按钮组的样式
.btn-container
    padding: 2px 0 0 0
	   display: flex
	   // border: 1px solid green
	   border-top: none
	.btn-container .el-button
    border-radius: 0
	  // margin-left: 0
  .btn-container .el-button+.el-button
    margin-left: 0px !important
    border-left: 1px solid #fff
   // 改变table高度时,多出gutter
.gutter
  display: none !important
  width: 0 !important
  // 右键菜单样式
.shortcut-container
      position: absolute
      left: 0
      top: 0
      width: 65px
      text-align: center
      line-height: 30px
      background: #fff
      border: 1px solid #dfdfdf
      padding:2px 0 4px 0
      cursor: pointer
      z-index: 9999999
      .el-button
        margin-left: 0
        width: 65px
        border: none
   // 搜索框样式
.search-container
    padding: 2px 20px 0 20px
    display: flex
// 留言板
.el-form-item .el-form-item__content .el-input .el-input__inner
    width: 100% !important